<!DOCTYPE html>
<html lang="pt-br">
    <%- include('partials/header.ejs') %>
<body>
    <h1><%= title %></h1>
    <%- include('partials/navbar.ejs') %>

    <ul> 
    <% users.forEach((user) => {%>
        <li>
            Nome: <%= user.name %> - Email: <%= user.email %> - Idade: <%= user.age %> -
            <a href="/edit?id=<%= user._id %>">[Editar]</a> -
            <a href="/remove/<%= user._id %>">[Apagar]</a>
        </li>
        
        <br>
    <% }) %>
    </ul>

    <%- include('partials/foot.ejs') %>
</body>
</html>